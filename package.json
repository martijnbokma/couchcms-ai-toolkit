{
    "name": "couchcms-ai-toolkit",
    "version": "1.5.0",
    "description": "Universal AI Development Toolkit for CouchCMS Projects",
    "type": "module",
    "main": "scripts/cli/sync.js",
    "bin": {
        "toolkit": "./bin/toolkit"
    },
    "engines": {
        "node": ">=18.0.0",
        "bun": ">=1.0.0"
    },
    "scripts": {
        "toolkit": "bun scripts/cli/toolkit.js",
        "install-toolkit": "bun scripts/cli/install.js",
        "sync": "bun scripts/cli/sync.js",
        "sync:watch": "bun scripts/cli/sync.js --watch",
        "validate": "bun scripts/cli/validate.js",
        "init": "bun scripts/cli/init.js",
        "create": "bun scripts/cli/create-standards.js",
        "migrate": "bun scripts/cli/migrate.js",
        "reinstall": "bun scripts/cli/reinstall.js",
        "health": "bun scripts/cli/health.js",
        "browse": "bun scripts/cli/browse.js",
        "browse:modules": "bun scripts/cli/browse.js --modules",
        "browse:agents": "bun scripts/cli/browse.js --agents",
        "update": "bun scripts/cli/update.js",
        "update:check": "bun scripts/cli/update.js --check",
        "update:apply": "bun scripts/cli/update.js --apply",
        "update-submodule": "bun scripts/utils/update-submodule.js",
        "prepare-contribution": "bun scripts/utils/prepare-contribution.js",
        "extend-modules": "bun scripts/maintenance/extend-modules.js",
        "create:module": "bun scripts/create/create-module.js",
        "create:agent": "bun scripts/create/create-agent.js",
        "git-flow": "bun scripts/git-flow/main.js",
        "git-flow-init": "bun scripts/git-flow/init.js",
        "release": "bun scripts/utils/quick-release.js",
        "release:auto": "bun scripts/utils/quick-release.js --auto",
        "release:major": "bun scripts/utils/quick-release.js --major",
        "release:minor": "bun scripts/utils/quick-release.js --minor",
        "release:patch": "bun scripts/utils/quick-release.js --patch",
        "audit:docs": "bun scripts/create/audit-docs.js",
        "build:web": "bun web/scripts/build.js",
        "build:web:fresh": "bun web/scripts/clean.js && bun web/scripts/build.js",
        "clean:web": "bun web/scripts/clean.js",
        "watch:web": "bun web/scripts/watch.js",
        "watch:web:server": "bun web/scripts/watch.js --server",
        "dev:web": "bun web/scripts/dev.js",
        "serve": "bun toolkit serve",
        "test": "bun test"
    },
    "dependencies": {
        "ansis": "^4.2.0",
        "gray-matter": "^4.0.3",
        "handlebars": "^4.7.8",
        "hono": "^4.10.7",
        "nunjucks": "^3.2.4",
        "yaml": "^2.8.2"
    },
    "devDependencies": {
        "fast-check": "^3.23.2",
        "tailwindcss": "^4.1.17",
        "@tailwindcss/cli": "^4.1.17",
        "daisyui": "^5.4.7"
    },
    "keywords": [
        "couchcms",
        "ai",
        "toolkit",
        "cursor",
        "claude",
        "copilot",
        "development"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/martijnbokma/couchcms-ai-toolkit.git"
    },
    "homepage": "https://github.com/martijnbokma/couchcms-ai-toolkit#readme",
    "bugs": {
        "url": "https://github.com/martijnbokma/couchcms-ai-toolkit/issues"
    },
    "author": {
        "name": "Martijn Bokma and all contributors"
    },
    "contributors": [
        {
            "name": "PixelPadThai",
            "url": "https://github.com/PixelPadThai"
        },
        {
            "name": "trendoman",
            "url": "https://github.com/trendoman"
        }
    ],
    "inspiration": [
        {
            "name": "aashari",
            "url": "https://github.com/aashari"
        },
        {
            "name": "diet103",
            "url": "https://github.com/diet103"
        }
    ],
    "license": "MIT"
}
